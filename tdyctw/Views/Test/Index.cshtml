
@{
    ViewBag.Title = "Index";
}

<h2>Tests</h2>

<canvas id="canvas" width="800" height="600" style="border: 1px solid black;"></canvas>

<script type="text/javascript">
    $(document).ready(function () {
        var canvas = document.getElementById("canvas");
        var ctx = canvas.getContext("2d");
        
        ctx.moveTo(0, 0);
        ctx.lineTo(200, 100);
        ctx.stroke();


    });
</script>

